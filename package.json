    "url": "https://github.com/pretur/pipeline/issues"
  },
  "homepage": "https://github.com/pretur/pipeline#readme",
  "devDependencies": {
    "chai": "^3.5.0",
    "mocha": "^2.4.5",
    "typescript": "^1.7.5",
    "typings": "^0.6.8"
  },
  "dependencies": {
    "bluebird": "^3.3.3",
    "esprima": "^2.7.2",
    "esprima-walk": "^0.1.0",
    "isomorphic-vm": "0.0.1"
  }
}
