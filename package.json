{
  "name": "pretur.pipeline",
  "version": "0.0.1",
  "description": "Monetary calculation pipeline",
  "main": "index.js",
  "files": [
    "*.js",
    "*.d.ts",
    "typings.json"
  ],
  "scripts": {
